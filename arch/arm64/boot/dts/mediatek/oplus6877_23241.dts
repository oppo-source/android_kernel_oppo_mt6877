&dsi0 {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	panel11@0 {
		compatible = "ac144,p_7,a0012,cmd";
		reg = <0>;
		reset-gpios = <&pio 40 0>;
		pinctrl-names = "default";
		port {
			panel_in11: endpoint {
				remote-endpoint = <&dsi_out>;
			};
		};
	};
	panel12@0 {
		compatible = "ac144,p_3,a0004,cmd";
		reg = <1>;
		reset-gpios = <&pio 40 0>;
		pinctrl-names = "default";
		port {
			panel_in12: endpoint {
				remote-endpoint = <&dsi_out>;
			};
		};
	};
	ports {
		port {
			dsi_out: endpoint {
				remote-endpoint = <&panel_in11>;
			};
		};
	};
};

