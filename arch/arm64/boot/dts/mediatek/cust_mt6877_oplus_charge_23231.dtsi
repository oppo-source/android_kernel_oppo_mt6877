#include "oplus_optiga_batt_def.h"
&odm {
	optiga {
		compatible = "oplus-optiga";
		status = "ok";
		pinctrl-names = "optiga_active";
		pinctrl-0 = <&optiga_active>;
		data-gpio = <&pio 140 0x00>;
		cpu-id = <7>;
		key-id = <1>;
		support_optiga_in_lk;
		barcode_read_support;
		/* vendor_index: 1st vendor or 2nd vendor.
		 * module_vendor: battery module vendor.
		 * core_vendor: battery core vendor.
		 * core_limited_vol: such as 4.48v battery core.
		 */
		oplus,optiga_batt_info = <FIRST_VENDOR  NVT       ATL      BAT_CORE_LIMITED_VOL_4480
					  FIRST_VENDOR  ATL_INDIA ATL      BAT_CORE_LIMITED_VOL_4480
					  FIRST_VENDOR  NVT       ATL      BAT_CORE_LIMITED_VOL_4480
					  FIRST_VENDOR  ATL_INDIA ATL      BAT_CORE_LIMITED_VOL_4480
					  SECOND_VENDOR GUANYU_M  GUANYU_C BAT_CORE_LIMITED_VOL_4480
					  FIRST_VENDOR  NVT       ATL      BAT_CORE_LIMITED_VOL_4480>;

		/* every barcode according to a line (vendor_index module_vendor core_vendor core_limited_vol).
		 * at most 6 barcode defined in battery driver oplus_optiga.h. (#define MAX_BARCODE_NUM 6).
		 * 611303000055 1st vendor. nvt battery barcode for dvt and later hardware version.
		 * 611303000056 1st vendor. atl india battery barcode for dvt and later hardware version.
		 * 611303000003 1st vendor. nvt battery barcode for evt and before hardware version.
		 * 611303000004 1st vendor. atl india battery barcode for evt and before hardware version.
		 * 611303000043 2nd vendor. guanyu battery barcode for all hardware version.
		 * 612903000002 fake battery.
		 */
		oplus,batt_barcode = "611303000055", "611303000056", "611303000003", "611303000004", "611303000043", "612903000002";
	};
};

