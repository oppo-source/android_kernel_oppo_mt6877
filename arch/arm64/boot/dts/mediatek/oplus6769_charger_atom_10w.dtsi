&i2c5 {
	status = "okay";
	clock-frequency = <400000>;//400K
	sgm41512_chg:sgm41512-charger@6b {
                status = "okay";
                compatible = "ti,sgm41512-charger";
                reg = <0x6b>;
                charger_name = "SGM41512";
                interrupt-parent = <&pio>;
                interrupts = <20 IRQ_TYPE_EDGE_FALLING 20 0>;
                chg-irq-gpio = <&pio 20 0x0>;
